{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": "(.js|.jsx)",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./jsdoc",
    "recurse": true,
    "template": "./template",
    "tutorials": "./tutorials",
    "readme": "./readme.md"
  }
}
